# LLM (ad blueprint)
LLM_MODEL=gpt-4o
LLM_STRUCTURED_OUTPUTS=true

# Vision analysis
VISION_MODEL=gpt-4o
VISION_MAX_FRAMES=10
VISION_TEMPERATURE=0.2
VISION_IMAGE_DETAIL=high
VISION_STRUCTURED_OUTPUTS=true

# Whisper transcription
WHISPER_MODEL=whisper-1
WHISPER_SAMPLE_RATE=48000
WHISPER_AUDIO_FORMAT=wav

# SDXL generation
SDXL_WIDTH=1024
SDXL_HEIGHT=1792
SDXL_STEPS=40
SDXL_GUIDANCE=7.0
# SDXL_SEED=12345
# SDXL_NEGATIVE_PROMPT=...

# Optional local depth-based parallax (no API cost, increases image->video quality)
ENABLE_DEPTH_PARALLAX=false
# Parallax quality: low | medium | high
PARALLAX_QUALITY=medium
MIDAS_MODEL=midas_v21_small_256
PARALLAX_AMPLITUDE=12
PARALLAX_VERTICAL=6
# MIDAS_DEVICE=cpu

# Motion safety (avoid clipping on pans/shakes)
MOTION_OVERSCAN=1.05
